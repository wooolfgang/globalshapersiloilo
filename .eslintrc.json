{
    "extends": "airbnb",
    "rules": {
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "_id"
                ]
            }
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint"
}